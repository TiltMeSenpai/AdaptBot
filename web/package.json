{
  "name": "web",
  "version": "0.0.1",
  "description": "Web interface for AdaptBot",
  "main": "templates/index.js",
  "scripts": {
    "build-js": "webpack src/index.jsx lib/bundle.js --progress --colors --module-bind 'jsx=babel-loader'",
    "build-css": "node-sass --include-path scss src/style.scss lib/style.css",
    "build-all": "npm run build-js && npm run build-css",
    "build-watch": "webpack src/index.jsx lib/bundle.js --progress --watch --colors --module-bind 'jsx=babel-loader' --module-bind 'scss=!style!css!sass'",
    "dev-server": "webpack-dev-server --inline --hot --module-bind 'jsx=babel-loader' --content-base www/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "^6.7.6",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "0.25.0",
    "materialize-css": "0.97.7",
    "node-sass": "3.10.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "redux": "3.6.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  }
}
